{
  "version": "1.0",
  "server": {
    "worker_threads": 1,
    "listen_address": "127.0.0.1",
    "listen_port": 8080
  },
  "upstreams": [
    {
      "name": "backend",
      "load_balancing": "round_robin",
      "backends": [{"host": "127.0.0.1", "port": 3001}]
    }
  ],
  "routes": [
    {"path": "/", "method": "GET", "upstream": "backend"},
    {"path": "/health", "method": "GET", "upstream": "backend"}
  ]
}
