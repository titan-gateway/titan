{
  "version": "benchmark-fixed",
  "server": {
    "listen_address": "0.0.0.0",
    "listen_port": 8443,
    "backlog": 1024,
    "worker_threads": 4,
    "tls_enabled": true,
    "tls_certificate_path": "/workspace/certs/cert.pem",
    "tls_private_key_path": "/workspace/certs/key.pem",
    "tls_alpn_protocols": ["h2", "http/1.1"]
  },
  "routes": [
    {
      "path": "/",
      "method": "GET",
      "handler_id": "root",
      "upstream": "backend",
      "priority": 10
    },
    {
      "path": "/small",
      "method": "GET",
      "handler_id": "small",
      "upstream": "backend",
      "priority": 10
    }
  ],
  "upstreams": [
    {
      "name": "backend",
      "load_balancing": "round_robin",
      "backends": [
        {
          "host": "127.0.0.1",
          "port": 3001,
          "weight": 1,
          "max_connections": 1000
        }
      ]
    }
  ]
}