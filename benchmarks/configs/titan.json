{
  "version": "benchmark",
  "server": {
    "listen_address": "0.0.0.0",
    "listen_port": 8080,
    "backlog": 1024,
    "worker_threads": 4
  },
  "routes": [
    {
      "path": "/small",
      "method": "GET",
      "handler_id": "small",
      "upstream": "backend",
      "priority": 10
    },
    {
      "path": "/medium",
      "method": "GET",
      "handler_id": "medium",
      "upstream": "backend",
      "priority": 10
    },
    {
      "path": "/large",
      "method": "GET",
      "handler_id": "large",
      "upstream": "backend",
      "priority": 10
    }
  ],
  "upstreams": [
    {
      "name": "backend",
      "load_balancing": "round_robin",
      "backends": [
        {
          "host": "127.0.0.1",
          "port": 3001,
          "weight": 1,
          "max_connections": 1000
        }
      ]
    }
  ]
}
